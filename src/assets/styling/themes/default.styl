@font-face
  font-family BookInsanity
  src url(../assets/styling/Bookinsanity.otf)

html
  background #ccc
  color #3a3f51
  font-family BookInsanity

h3
  border-bottom 2px solid #c9ad6a

h1, h2, h3, h4, h5, h6
  color #4E2A84

a, a:visited
  color #813ce8

tr:first-child
  background-color #f5f5f5

tr, td
  border 1px #4E2A84 solid
  
#sidebar
  background-color #efe4f5

#sidebarToggle
  background-color #4E2A84

#arrow
  border solid #ccc
